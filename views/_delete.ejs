<div class="form-group row">
  <label class="col-form-label col-sm-2" for="username">Username</label>
  <div class="col-sm-8">
    <input
      readonly
      class="form-control"
      id="username"
      value="<%= model.username %>"
    />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="email">Email</label>
  <div class="col-sm-7">
    <input
      readonly
      class="form-control"
      id="email"
      value="<%= model.email %>"
    />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="bio">Bio</label>
  <div class="col-sm-10">
    <textarea
      readonly
      class="form-control"
      cols="20"
      id="bio"
      maxlength="32000"
      rows="7"
    >
<%= model.bio %></textarea
    >
  </div>
</div>
